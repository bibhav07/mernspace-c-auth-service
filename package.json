{
    "name": "auth-service",
    "version": "1.0.0",
    "description": "A user management service",
    "main": "server.ts",
    "scripts": {
        "dev": "cross-env NODE_ENV=dev nodemon -L src/server.ts",
        "format:check": "prettier . --check",
        "format:fix": "prettier . --write",
        "lint:check": "eslint .",
        "lint:fix": "eslint . --fix",
        "prepare": "husky install",
        "test": "jest --watch --runInBand",
        "start": "ts-node src/index.ts",
        "typeorm": "typeorm-ts-node-commonjs"
    },
    "author": "bibhav k yadav",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/http-errors": "^2.0.4",
        "@types/jest": "^29.5.11",
        "@types/node": "^16.11.10",
        "@types/supertest": "^2.0.16",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.55.0",
        "eslint-config-prettier": "^9.1.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.0",
        "nodemon": "^3.0.2",
        "prettier": "3.1.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "10.7.0",
        "typescript": "4.5.2"
    },
    "lint-staged": {
        "*.ts": [
            "npm run lint:fix",
            "npm run format:fix"
        ]
    },
    "dependencies": {
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "http-errors": "^2.0.0",
        "pg": "^8.4.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "0.3.17"
    }
